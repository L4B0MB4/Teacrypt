{
  "name": "Teacrypt",
  "description": "mydescription",
  "version": "2.0",
  "permissions": ["tabs", "activeTab"],
  "browser_action": {
    "default_title": "Teacrypt",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": { "page": "popup.html" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["injected.js"],
  "manifest_version": 2
}
